@page
@inject IViewLocalizer Localizer
@{
    ViewData["Title"] = Localizer["title"];
}
<section role="contentinfo">
    <nav role="navigation" id="wb-bc" property="breadcrumb">
        <h2>
            @Localizer["You are here:"]
        </h2>
        <div class="row">
            <ol class="breadcrumb">
                <li><a asp-page="../../../Index">@Localizer["Home"]</a></li>
                <li><a asp-page="../../Index">@Localizer["Candidate"]</a></li>
                <li><a asp-page="../Index">@Localizer["Seagoing"]</a></li>
                <li><a class="breadcrumb-item active" aria-current="page">@Localizer["Crew"]</a></li>
            </ol>
        </div>
    </nav>
    <nav role="navigation">
        <h2 class="wb-inv">Document navigation</h2>
        <ul class="pager">
            <li class="previous"><a asp-page="../Index" class="btn btn-default" type="button" rel="prev">@Localizer["Previous"]</a></li>
        </ul>
    </nav>
    <div class="cct-External-Crew-img cct-External-Crew-hide-mobile cct-External-Crew-hide-tablet">
        <div class="container">
            <div class="row mrgn-tp-lg mrgn-bttm-lg">
                <div class="col-md-7">
                    <h2>@Localizer["Q1"]</h2>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>

   <div class="row">
        <div class="col-sm-12">
            <ul class="list">
                <li><a alt="@Localizer["searchdeck"]" style="text-decoration:none" asp-page="Deck/PositionList">@Localizer["Deck"]</a></li>
                <li><a alt="@Localizer["searchengineering"]" style="text-decoration:none" asp-page="Engineering/PositionList">@Localizer["Engineering"]</a></li>
                <li><a alt="@Localizer["searchlogistics"]" style="text-decoration:none" asp-page="Logistics/PositionList">@Localizer["Logistics"]</a></li>
            </ul>
        </div>
    </div>
</section>

