@page
@inject IViewLocalizer Localizer
@{
    ViewData["Title"] = Localizer["title"];
}
    <section role="contentinfo">
        <nav role="navigation" id="wb-bc" property="breadcrumb">
            <h2>
                @Localizer["You are here:"]
            </h2>
            <div class="row">
                <ol class="breadcrumb">
                    <li><a href="../../Index">@Localizer["Home"]</a></li>
                    <li><a href="../Index">@Localizer["Candidate"]</a></li>
                    <li><a class="breadcrumb-item active" aria-current="page">@Localizer["Seagoing"]</a></li>
                </ol>
            </div>
        </nav>
        <nav role="navigation">
            <h2 class="wb-inv">Document navigation</h2>
            <ul class="pager">
                <li class="previous"><a asp-page="../Index" class="btn btn-default" type="button" rel="prev">@Localizer["Previous"]</a></li>
            </ul>
        </nav>
        <h2>@Localizer["Q1"]</h2>
        <div class="row">
            <div class="col-sm-6">
                <figure>
                    <a style="text-decoration:none" class="nav-link" asp-page="Officer/Index">
                        <figcaption><h4 class="text-center">@Localizer["Officer"]</h4></figcaption>
                        <img src="~/img/CCGMember/SO.jpg" alt="@Localizer["PhotoOfficer"]" class="img-thumbnail img-responsive mrgn-bttm-sm" />
                    </a>
                </figure>
            </div>
            <div class="col-sm-6">
                <figure>
                    <a style="text-decoration:none" class="nav-link" asp-page="Crew/Index">
                        <figcaption><h4 class="text-center">@Localizer["Crew"]</h4></figcaption>
                        <img src="~/img/CCGMember/SC.jpg" alt="@Localizer["PhotoCrew"]" class="img-thumbnail img-responsive mrgn-bttm-sm" />
                    </a>
                </figure>
            </div>
        </div>

        <!-- <div style="clear:both;">
            <div class="col-lg-12">
                <nav role="navigation">
                    <h2 class="wb-inv">Document navigation</h2>
                    <ul class="pager">
                        <li class="previous"><a asp-page="../Index" class="btn btn-default" type="button" rel="prev">@Localizer["Previous"]</a></li>
                    </ul>
                </nav>
            </div>
        </div>
    -->
    </section>

