@page
@model Web.Pages.Student.HighSchool.AllPositionsModel
@inject IViewLocalizer Localizer
@{
    ViewData["Title"] = "AllPositions";
}

    <section role="contentinfo">

        <h2>@Localizer["Q1"]</h2>

        <div class="col-lg-4 col-sm-6">
            <figure class="thumbnail eqht-trgt" style="vertical-align: top;">
                <div class="img-container">
                    <a href="@Localizer["link"]" target="_blank">
                        <img src="~/img/Student/CCGCollege.jpg" alt="Photo of" class="img-responsive" />
                    </a>
                </div>
                <figcaption class="caption text-center"><h4 class="h6 mrgn-tp-sm"><a style="text-decoration:none" href="@Localizer["link"]" target="_blank">@Localizer["ccgcollege"]</a></h4></figcaption>
            </figure>
        </div>

        <div class="col-lg-4 col-sm-6">
            <figure class="thumbnail eqht-trgt" style="vertical-align: top;">
                <div class="img-container">
                    <a asp-page="Administrative">
                        <img src="~/img/CCGMember/AS.jpg" alt="Photo of" class="img-responsive" />
                    </a>
                </div>
                <figcaption class="caption text-center"><h4 class="h6 mrgn-tp-sm"><a style="text-decoration:none" href="HS_Admin_List">@Localizer["admin"]</a></h4></figcaption>
            </figure>
        </div>

        <div class="col-lg-4 col-sm-6">
            <figure class="thumbnail eqht-trgt" style="vertical-align: top;">
                <div class="img-container">
                    <a asp-page="Computer">
                        <img src="~/img/CCGMember/CS.jpg" alt="Photo of" class="img-responsive" />
                    </a>
                </div>
                <figcaption class="caption text-center"><h4 class="h6 mrgn-tp-sm"><a style="text-decoration:none" href="HS_Computer_List">@Localizer["computer"]</a></h4></figcaption>
            </figure>
        </div>

        <div class="col-lg-4 col-sm-6">
            <figure class="thumbnail eqht-trgt" style="vertical-align: top;">
                <div class="img-container">
                    <a asp-page="Trades">
                        <img src="~/img/CCGMember/GL.jpg" alt="Photo of" class="img-responsive" />
                    </a>
                </div>
                <figcaption class="caption text-center"><h4 class="h6 mrgn-tp-sm"><a style="text-decoration:none" href="HS_Trades_List">@Localizer["trades"]</a></h4></figcaption>
            </figure>
        </div>

        <div class="clearfix"></div>
        <div class="row">
            <div class="col-lg-12 mrgn-bttm-md">
                <nav role="navigation" class="mrgn-bttm-lg">
                    <h2 class="wb-inv">Document navigation</h2>
                    <ul class="pager">
                        <li class="previous"><a asp-page="../Index" class="btn btn-default" type="button" rel="prev">@Localizer["Previous"]</a></li>
                    </ul>
                </nav>
            </div>
        </div>
    </section>

