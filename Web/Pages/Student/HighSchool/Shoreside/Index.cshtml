@page
@model Web.Pages.Student.HighSchool.Shoreside.IndexModel
@inject IViewLocalizer Localizer
@{
    ViewData["Title"] = Localizer["Shoreside"];
}

    <section role="contentinfo">
        <section role="contentinfo">
            <nav role="navigation" id="wb-bc" property="breadcrumb">
                <h2>
                    @Localizer["You are here:"]
                </h2>
                <div class="row">
                    <ol class="breadcrumb">
                        <li><a href="../../../Index">@Localizer["Home"]</a></li>
                        <li><a href="../../Index">@Localizer["Student"]</a></li>
                        <li><a href="../Index">@Localizer["High School"]</a></li>
                        <li><a class="breadcrumb-item active" aria-current="page">@Localizer["Shoreside"]</a></li>
                    </ol>
                </div>
            </nav>
            <nav role="navigation">
                <h2 class="wb-inv">Document navigation</h2>
                <ul class="pager">
                    <li class="previous"><a asp-page="../Index" class="btn btn-default" type="button" rel="prev">@Localizer["Previous"]</a></li>
                </ul>
            </nav>

            <h2>@Localizer["Q1"]</h2>

            <div class="col-lg-4 col-sm-6">
                <figure class="thumbnail eqht-trgt" style="vertical-align: top;">
                    <div class="img-container">
                        <a style="text-decoration:none" href="@Localizer["link"]" target="_blank">
                            <figcaption class="caption text-center"><h4 class="h6 mrgn-tp-sm">@Localizer["ccgcollege"]</h4></figcaption>
                            <img src="~/img/Student/CCGCollege.jpg" alt="@Localizer["PhotoCollege"]" class="img-responsive" />
                        </a>
                    </div>
                </figure>
            </div>

            <div class="col-lg-4 col-sm-6">
                <figure class="thumbnail eqht-trgt" style="vertical-align: top;">
                    <div class="img-container">
                        <a style="text-decoration:none" class="nav-link" asp-page="Administrative">
                            <figcaption class="caption text-center"><h4 class="h6 mrgn-tp-sm">@Localizer["admin"]</h4></figcaption>
                            <img src="~/img/CCGMember/Admin.jpg" alt="@Localizer["PhotoAdmin"]" class="img-responsive" />
                        </a>
                    </div>
                </figure>
            </div>

            <div class="col-lg-4 col-sm-6">
                <figure class="thumbnail eqht-trgt" style="vertical-align: top;">
                    <div class="img-container">
                        <a style="text-decoration:none" class="nav-link" asp-page="Computer">
                            <figcaption class="caption text-center"><h4 class="h6 mrgn-tp-sm">@Localizer["computer"]</h4></figcaption>
                            <img src="~/img/CCGMember/CS.jpg" alt="@Localizer["PhotoComputer"]" class="img-responsive" />
                        </a>
                    </div>
                </figure>
            </div>

            <div class="col-lg-4 col-sm-6">
                <figure class="thumbnail eqht-trgt" style="vertical-align: top;">
                    <div class="img-container">
                        <a style="text-decoration:none" class="nav-link" asp-page="Trades">
                            <figcaption class="caption text-center"><h4 class="h6 mrgn-tp-sm">@Localizer["trades"]</h4></figcaption>
                            <img src="~/img/CCGMember/GL.jpg" alt="@Localizer["PhotoTrades"]" class="img-responsive" />
                        </a>
                    </div>
                   
                </figure>
            </div>

