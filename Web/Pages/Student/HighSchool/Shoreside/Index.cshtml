@page
@model Web.Pages.Student.HighSchool.Shoreside.IndexModel
@inject IViewLocalizer Localizer
@{
    ViewData["Title"] = Localizer["Shoreside"];
}


<section role="contentinfo">
    <nav role="navigation" id="wb-bc" property="breadcrumb">
        <h2 class="wb-inv">
            @Localizer["You are here:"]
        </h2>
        <div class="row">
            <ol class="breadcrumb">
                <li><a asp-page="../../../Index">@Localizer["Home"]</a></li>
                <li><a asp-page="../../Index">@Localizer["Student"]</a></li>
                <li><a asp-page="../Index">@Localizer["High School"]</a></li>
                <li><a class="breadcrumb-item active" aria-current="page">@Localizer["Shoreside"]</a></li>
            </ol>
        </div>
    </nav>
    <nav role="navigation">
        <h2 class="wb-inv">Document navigation</h2>
        <ul class="pager">
            <li class="previous"><a asp-page="../Index" class="btn btn-default" type="button" rel="prev">@Localizer["Previous"]</a></li>
        </ul>
    </nav>
    <div class="cct-HS-shore-img cct-HS-shore-hide-mobile cct-HS-shore-hide-tablet">
        <div class="container">
            <div class="row mrgn-tp-lg mrgn-bttm-lg">
                <div class="col-md-7">
                    <h2>@Localizer["Q1"]</h2>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>
    
    <div class="row">
        <div class="col-sm-12">
            <ul class="list">
                <li><a href="@Localizer["link"]" target="_blank">@Localizer["ccgcollege"] <span class="glyphicon glyphicon-new-window"></span></a></li>
                <li><a asp-page="Administrative">@Localizer["admin"]</a></li>
                <li><a asp-page="Computer">@Localizer["computer"]</a></li>
                <li><a asp-page="Trades">@Localizer["trades"]</a></li>
            </ul>
        </div>
    </div>
</section>
