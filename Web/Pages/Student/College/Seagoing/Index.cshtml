@page
@model Web.Pages.Student.College.Seagoing.IndexModel
@inject IViewLocalizer Localizer
@{
    ViewData["Title"] = Localizer["Seagoing"];
}
<section role="contentinfo">
    <nav role="navigation" id="wb-bc" property="breadcrumb">
        <h2 class="wb-inv">
            @Localizer["You are here:"]
        </h2>
        <div class="row">
            <ol class="breadcrumb">
                <li><a asp-page="../../../Index">@Localizer["Home"]</a></li>
                <li><a asp-page="../../Index">@Localizer["Student"]</a></li>
                <li><a asp-page="../Index">@Localizer["TechCollege"]</a></li>
                <li><a class="breadcrumb-item active" aria-current="page">@Localizer["Seagoing"]</a></li>
            </ol>
        </div>
    </nav>
    <nav role="navigation">
        <h2 class="wb-inv">Document navigation</h2>
        <ul class="pager">
            <li class="previous"><a asp-page="../Index" class="btn btn-default" type="button" rel="prev">@Localizer["Previous"]</a></li>
        </ul>
    </nav>

    <div class="cct-collegesea-img cct-collegesea-hide-mobile cct-collegesea-hide-tablet">
        <div class="container">
            <div class="row">
                <div class="col-md-7">
                    <h2 class="mrgn-bttm-xl">@Localizer["Q1"]</h2>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>

    <div class="row mrgn-tp-md">
        <div class="col-sm-12">
            <ul class="list">
                <li><a style="text-decoration:none" asp-page="Officer/Index">@Localizer["Officer"]</a></li>
                <li><a style="text-decoration:none" asp-page="Crew/Index">@Localizer["Crew"]</a></li>
            </ul>
        </div>
    </div>
</section>
